package com.example.myapplication

import android.os.Bundle
import android.view.LayoutInflater
import android.view.View
import android.view.ViewGroup
import androidx.fragment.app.Fragment
import com.example.myapplication.databinding.FragmentImageGalleryBinding

class ImageGalleryFragment : Fragment() {

    private var _binding: FragmentImageGalleryBinding? = null
    private val binding get() = _binding!!

    override fun onCreateView(
        inflater: LayoutInflater, container: ViewGroup?,
        savedInstanceState: Bundle?
    ): View? {
        _binding = FragmentImageGalleryBinding.inflate(inflater, container, false)
        val view = binding.root

        // Initialize your image gallery logic here

        return view
    }

    override fun onDestroyView() {
        super.onDestroyView()
        _binding = null
    }
}
